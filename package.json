{
	"name": "spur-live-chat-agent",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"type": "module",
	"scripts": {
		"check": "biome check --write .",
		"dev": "turbo dev",
		"build": "turbo build",
		"start": "cd apps/server && pnpm run start",
		"check-types": "turbo check-types",
		"dev:native": "turbo -F native dev",
		"dev:web": "turbo -F web dev",
		"dev:server": "turbo -F server dev",
		"db:push": "turbo -F @spur-live-chat-agent/db db:push",
		"db:studio": "turbo -F @spur-live-chat-agent/db db:studio",
		"db:generate": "turbo -F @spur-live-chat-agent/db db:generate",
		"db:migrate": "turbo -F @spur-live-chat-agent/db db:migrate",
		"db:local": "turbo -F @spur-live-chat-agent/db db:local",
		"db:seed": "turbo -F @spur-live-chat-agent/db db:seed"
	},
	"dependencies": {
		"dotenv": "catalog:",
		"zod": "catalog:",
		"@spur-live-chat-agent/env": "workspace:*"
	},
	"devDependencies": {
		"turbo": "^2.6.3",
		"@biomejs/biome": "^2.2.0",
		"typescript": "catalog:",
		"@types/node": "catalog:",
		"@spur-live-chat-agent/config": "workspace:*"
	},
	"packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}
